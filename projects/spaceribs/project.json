{
  "name": "spaceribs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "projects/spaceribs/src",
  "prefix": "spaceribs",
  "targets": {
    "build": {
      "executor": "@angular-builders/custom-webpack:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "customWebpackConfig": {
          "path": "projects/spaceribs/webpack.config.js"
        },
        "outputPath": "dist/projects/spaceribs",
        "index": "projects/spaceribs/src/index.html",
        "main": "projects/spaceribs/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "projects/spaceribs/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "projects/spaceribs/src/favicon.ico",
          "projects/spaceribs/src/.nojekyll",
          "projects/spaceribs/src/assets"
        ],
        "styles": ["projects/spaceribs/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-builders/custom-webpack:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "spaceribs:build:production"
        },
        "development": {
          "browserTarget": "spaceribs:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "spaceribs:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "projects/spaceribs/**/*.ts",
          "projects/spaceribs/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "projects/spaceribs/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
