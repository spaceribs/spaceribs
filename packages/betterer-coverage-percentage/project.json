{
  "name": "betterer-coverage-percentage",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/betterer-coverage-percentage/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/betterer-coverage-percentage",
        "main": "packages/betterer-coverage-percentage/src/index.ts",
        "tsConfig": "packages/betterer-coverage-percentage/tsconfig.lib.json",
        "assets": ["packages/betterer-coverage-percentage/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/betterer-coverage-percentage/jest.config.ts"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "postTargets": [
          "betterer-coverage-percentage:github",
          "betterer-coverage-percentage:npm"
        ]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "npm": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "typedoc": {
      "executor": "@enio.ai/typedoc:typedoc",
      "outputs": ["{workspaceRoot}/docs/packages/betterer-coverage-percentage"],
      "options": {
        "options": "typedoc.json"
      },
      "configurations": {}
    }
  },
  "tags": []
}
